{% extends "layouts/base.html" %}

{% block title %}Regatten - {{event.name}}{% endblock %}

{% block content %}
  <div class=container>
    <h1>Ergebnisse</h1>
    <p>
      <ul>
        {% for result in results %}
          <li><a href="/events/{{event.id}}/finals/{{result.finalId}}">{{ result.raceName }} - {{ result.finalName }}</a>
        {% empty %}
          <li>Es liegen keine Ergebnisse vor</li>
        {% endfor %}
      </ul>
    </p>
  </div>

{% endblock  %}
