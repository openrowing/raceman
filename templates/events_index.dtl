{% extends "layouts/base.html" %}

{% block title %}Regatten{% endblock %}

{% block content %}

  <div class=container>
    <h1>Regatten</h1>
    <p>
      <ul>
        {% for event in events %}
          <li><a href="/events/{{event.id}}">{{ event.name }}</a> ({{ event.year }} in {{ event.venueCity }}/{{ event.venueCountry }})</li>
        {% empty %}
          <li>Keine Regatten aktiv</li>
        {% endfor %}
      </ul>
    </p>
  </div>

{% endblock  %}
